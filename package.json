{
  "name": "fittracker",
  "main": "./index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "npx @expo/cli start",
    "start:monitored": "start-app-with-monitor.bat",
    "monitor": "start-monitor.bat",
    "monitor:quick": "powershell -ExecutionPolicy Bypass -File quick-monitor.ps1",
    "monitor:with-expo": "powershell -ExecutionPolicy Bypass -File quick-monitor.ps1 -StartExpo",
    "monitor:stop": "powershell -Command \"Get-Process | Where-Object { $_.ProcessName -like '*expo*' -or $_.ProcessName -like '*node*' } | Stop-Process -Force\"",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx @expo/cli run:android",
    "ios": "npx @expo/cli run:ios",
    "web": "npx @expo/cli start --web",
    "lint": "npx @expo/cli lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "expo": "53.0.12",
    "expo-background-fetch": "^13.1.5",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-file-system": "^18.1.10",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-location": "^18.1.5",
    "expo-sensors": "^14.1.4",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.9",
    "expo-task-manager": "^13.1.5",
    "expo-web-browser": "~14.2.0",
    "nanoid": "^5.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-health": "^1.19.0",
